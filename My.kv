<WidgetExample>:
    cols:3
    ToggleButton:
        text:'dont click'
        on_state:root.noclick(self)
    Button:
        text:'click here'
        on_press:root.click()
        disabled:root.enabled
    BoxLayout:
        orientation:'vertical'
        Label:
            text:str(round(my_slider.value))
            font_size:'30dp'
        ProgressBar:
            max:100
            value:44
    TextInput:
        id:ii
        text:'text'
        size_hint:None, 1
        width:'200dp'
        multiline:False
        on_text_validate:root.validation(self)
    Label:
        text:root.valide_text
        size_hint:None, 1
        width:'200dp'
    Switch:
        id:my_switch
        on_active:root.switched(self)
        size_hint:None, 1
        width:'100dp'
    Slider:
        disabled:my_switch.active
        id: my_slider
        min:0
        max:345
        value:156
        on_value:root.change(self)


